<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="WaterSource.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <log4net>
    <appender name="MainFileAppender" type="Utils.FileAppender">
      <appendToFile value="false"/>
      <layout type="log4net.Layout.PatternLayout">
        <header value=""/>
        <footer value=""/>
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>

    <root>
      <level value="DEBUG" />
      <appender-ref ref="MainFileAppender" />
    </root>

    <logger name="CommSub.Communicator">
      <level value="DEBUG" />
    </logger>

    <logger name="AppShared.GameProcess_Deep">
      <level value="INFO" />
    </logger>

    <logger name="AppShared.GameProcessForm">
      <level value="INFO" />
    </logger>

    <logger name="Utils">
      <level value="INFO" />
    </logger>

  </log4net>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <applicationSettings>
    <WaterSource.Properties.Settings>
        <setting name="Registry" serializeAs="String">
            <value>127.0.0.1:12000</value>
        </setting>
        <setting name="RegistryEndPoint" serializeAs="String">
            <value>127.0.0.1:12000</value>
        </setting>
        <setting name="MaxPort" serializeAs="String">
            <value>12999</value>
        </setting>
        <setting name="MaxRetries" serializeAs="String">
            <value>3</value>
        </setting>
        <setting name="Timeout" serializeAs="String">
            <value>3000</value>
        </setting>
        <setting name="MinPort" serializeAs="String">
            <value>12001</value>
        </setting>
        <setting name="ProcessName" serializeAs="String">
            <value>Balloon Store</value>
        </setting>
        <setting name="AutoStart" serializeAs="String">
            <value>True</value>
        </setting>
        <setting name="ANumber" serializeAs="String">
            <value>A00000001</value>
        </setting>
        <setting name="FirstName" serializeAs="String">
            <value>The</value>
        </setting>
        <setting name="LastName" serializeAs="String">
            <value>Instructor</value>
        </setting>
        <setting name="Alias" serializeAs="String">
            <value>Instructor</value>
        </setting>
    </WaterSource.Properties.Settings>
  </applicationSettings>
</configuration>